t_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">CREATE TRIGGER TRGtrimestre
BEFORE INSERT ON fichainstructor
FOR EACH ROW
WHEN NEW.TRIMESTRE IS NULL OR NEW.TRIMESTRE = 0
BEGIN
    SELECT
        CASE
            WHEN CAST(strftime('%m', COALESCE(NEW.fecha, DATE('now'))) AS INTEGER) BETWEEN 1 AND 3 THEN
                NEW.TRIMESTRE := 1
            WHEN CAST(strftime('%m', NEW.fecha) AS INTEGER) BETWEEN 4 AND 6 THEN
                NEW.TRIMESTRE := 2
            WHEN CAST(strftime('%m', NEW.fecha) AS INTEGER) BETWEEN 7 AND 9 THEN
                NEW.TRIMESTRE := 3
            ELSE
                NEW.TRIMESTRE := 4
        END;
END;
